{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "modules": false,
        "exclude": ["transform-regenerator", "transform-async-to-generator"]
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "h",
        "pragmaFrag": "Fragment",
        "throwIfNamespace": false
      }
    ],
    [
      "module:fast-async",
      {
        "spec": "true"
      }
    ]
  ]
}
