{
  "presets": [
    [
      "@babel/preset-env",
      {
        "loose": true,
        "modules": "auto"
      }
    ]
  ],
  "plugins": [
    [
      "@babel/plugin-transform-react-jsx",
      {
        "pragma": "h",
        "pragmaFrag": "Fragment",
        "throwIfNamespace": false
      }
    ],
    ["emotion"]
  ]
}
