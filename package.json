{
  "name": "@mobility-platform/global-navigation",
  "version": "5.1.0",
  "main": "dist/main.js",
  "license": "CC-BY-ND-4.0",
  "repository": "mobility-platform/global-navigation",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack -p",
    "start": "webpack-dev-server",
    "prepublishOnly": "yarn build",
    "postversion": "git push --all && git push --tag",
    "test": "is-ci && jest --ci || jest --watch",
    "lint": "eslint \"src/**/*.+(js|jsx|json)\"",
    "format": "prettier --write \"src/**/*.+(js|jsx|json)\""
  },
  "dependencies": {
    "@emotion/cache": "^10.0.29",
    "@emotion/core": "^10.0.35",
    "@emotion/styled-base": "^10.0.31",
    "jwt-decode": "^2.2.0",
    "preact": "^10.4.8",
    "preact-habitat": "^3.3.0",
    "stylis-plugin-extra-scope": "^0.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@testing-library/preact": "^2.0.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "cross-env": "^7.0.2",
    "dotenv-webpack": "^2.0.0",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.0.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "html-webpack-plugin": "^4.4.1",
    "is-ci": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "browserslist": [
    "IE >= 11"
  ],
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "moduleNameMapper": {
      "^react$": "preact/compat",
      "^react-dom$": "preact/compat",
      "^react-dom/test-utils$": "preact/test-utils"
    }
  }
}
